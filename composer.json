{
	"name": "stonedz/pff2-installers",
	"type": "composer-plugin",
	"description": "Composer installers for pff2",
	"license": "MIT",
	"keywords": [
		"pff2", "module"
	],
	"authors": [
		{
			"name": "Paolo Fagni",
			"email": "paolo.fagni@gmail.com",
			"role": "Developer"
		}
	],
	"homepage": "https://github.com/stonedz/pff2-installers",
	"support": {
		"source": "https://github.com/stonedz/pff2-installers"
	},
	"autoload": {
		"psr-0": {
			"Composer\\Installer": "src/"
		}
	},
	"extra": {
		"class": "Composer\\Installer\\Pff2Installer"
	},
    "require": {
        "composer-plugin-api": "^1.0 || ^ 2.0"
    },
	"require-dev": {
		"composer/composer": "*"
	}
}
